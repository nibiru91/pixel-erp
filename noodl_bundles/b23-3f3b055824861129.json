appings based on the selected template component.\nmap({\n\t'supid':'supid',\n\t'date': 'date',\n\t'importo': 'importo',\n\t'soggetto': 'soggetto_ragsoc',\n\t'area': 'an_area_nome',\n\t'ticket': 'an_tecnologia_nome',\n\t'gestore': 'an_gestore_ragsoc',\n\t'prodotto': 'prodotto_nome',\n})"},"ports":[],"children":[]}]}]}]}]},{"id":"781ed96f-2c09-e789-d95a-20346c5c2f98","type":"Expression","parameters":{"expression":"\"Totale da Incassare Euro \" + importo.toFixed(2)"},"ports":[],"children":[]},{"id":"1b483ea7-f612-17fd-573d-32f9ddbadbdd","type":"JavaScriptFunction","parameters":{"functionScript":"const supabase = Noodl.Variables.supabase;\n\nif (!supabase) throw new Error('Supabase client not found');\n\n\n\ntry {\n  \n  const { data, error } = await supabase\n        .from('soggetti')\n        .select('id, ragione_sociale')\n        .eq('tipo', 1);\n\n    if (error) {\n    console.error(error)\n    Outputs.Failure();\n    }\n    \n    else {\n    // Trasforma i risultati in un formato { Label, Value }\n    const transformedData = data.map(soggetto => ({\n        Value: soggetto.id.toString(),\n        Label: soggetto.ragione_sociale\n    }));\n\n    //return transformedData;\n\n    Outputs.Data = data;\n    Outputs.TransformedData = transformedData;\n    Outputs.Success();\n\n  }\n} \n  catch (error) {\n    console.error(error)\n    Outputs.Failure();\n  }"},"ports":[{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":4},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":5},{"name":"out-Data","displayName":"Data","plug":"output","type":"*","group":"Outputs","index":6},{"name":"out-TransformedData","displayName":"TransformedData","plug":"output","type":"*","group":"Outputs","index":7}],"children":[]},{"id":"08e066ea-d605-4135-4341-5cea602a13f1","type":"JavaScriptFunction","parameters":{"functionScript":"const supabase = Noodl.Variables.supabase;\n\nif (!supabase) throw new Error('Supabase client not found');\n\n\n\ntry {\n  \n  const { data, error } = await supabase\n        .from('def_an_tecnologia')\n        .select('id, nome');\n\n    if (error) {\n    console.error(error)\n    Outputs.Failure();\n    }\n    \n    else {\n    // Trasforma i risultati in un formato { Label, Value }\n    const transformedData = data.map(tecnologia => ({\n        Value: tecnologia.id.toString(),\n        Label: tecnologia.nome\n    }));\n\n    //return transformedData;\n\n    Outputs.Data = data;\n    Outputs.TransformedData = transformedData;\n    Outputs.Success();\n\n  }\n} \n  catch (error) {\n    console.error(error)\n    Outputs.Failure();\n  }"},"ports":[{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":4},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":5},{"name":"out-Data","displayName":"Data","plug":"output","type":"*","group":"Outputs","index":6},{"name":"out-TransformedData","displayName":"TransformedData","plug":"output","type":"*","group":"Outputs","index":7}],"children":[]},{"id":"581ecb81-8568-3c19-3b7f-2902b6d347a3","type":"JavaScriptFunction","parameters":{"functionScript":"const supabase = Noodl.Variables.supabase;\n\nif (!supabase) throw new Error('Supabase client not found');\n\n\n\ntry {\n  \n  const { data, error } = await supabase\n        .from('def_an_area')\n        .select('id, nome');\n\n    if (error) {\n    console.error(error)\n    Outputs.Failure();\n    }\n    \n    else {\n    // Trasforma i risultati in un formato { Label, Value }\n    const transformedData = data.map(area => ({\n        Value: area.id.toString(),\n        Label: area.nome\n    }));\n\n    //return transformedData;\n\n    Outputs.Data = data;\n    Outputs.TransformedData = transformedData;\n    Outputs.Success();\n\n  }\n} \n  catch (error) {\n    console.error(error)\n    Outputs.Failure();\n  }"},"ports":[{"name":"out-Failure","displayName":"Failure","plug":"output","type":"signal","group":"Outputs","index":4},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":5},{"name":"out-Data","displayName":"Data","plug":"output","type":"*","group":"Outputs","index":6},{"name":"out-TransformedData","displayName":"TransformedData","plug":"output","type":"*","group":"Outputs","index":7}],"children":[]},{"id":"26b658d7-d8f0-b972-b476-02f14bd12cb2","type":"Component Inputs","parameters":{},"ports":[],"children":[]},{"id":"d3b55bd4-4b72-e9be-1ebd-8329787eafe1","type":"JavaScriptFunction","parameters":{"functionScript":"ricavi = Inputs.ricavi;\n\n// Mappiamo i ricavi per diminuire le dimensioni del dataset ai soli dati utili, che sono già filtrati\nlet ricaviModificati = ricavi.map(item => {\n    return {\n        data: item.date,\n        importo: item.importo,  // Moltiplica l'importo per -1\n    };\n});\n\nOutputs.ricavi = ricaviModificati;\nOutputs.Success();\n\n"},"ports":[{"name":"in-ricavi","displayName":"ricavi","plug":"input","type":"*","group":"Inputs","index":4},{"name":"out-Success","displayName":"Success","plug":"output","type":"signal","group":"Outputs","index":5},{"name":"out-ricavi","displayName":"ricavi","plug":"output","type":"*","group":"Outputs","index":6}],"children":[]},{"id":"c91bed0a-cd3a-500f-770d-030fa67f2d9d","type":"JavaScriptFunction","parameters":{"functionScript":"const array = Inputs.Ricavi;  // Assumiendo l'array di oggetti completo\n\n// Oggetto per tenere traccia degli importi aggregati per mese\nconst totaliPerMese = {};\n\n// Aggrega gli importi per mese\narray.forEach(item => {\nconst mese = item.data.substring(0, 7); // Estrae l'anno e il mese dalla data\nif (totaliPerMese[mese]) {\n    totaliPerMese[mese] += item.importo;\n} else {\n    totaliPerMese[mese] = item.importo;\n}\n});\n\n// Preparare i dati per Chart.js\nconst labels = Object.keys(totaliPerMese); // Etichette dei mesi\nconst data = Object.values(totaliPerMese); // Dati degli importi\n\nconst datiGrafico = {\n    labels,\n    datasets: [\n        {\n        label: 'Totali Mensili',\n        data: data,\n        borderColor: 'blue',\n        backgroundColor: 'blue',\n        }\n    ]\n};\n\nOutputs.Data = datiGrafico;\n\nconst config = {\n            y: {\n                grid: {\n                    zeroLineWidth: 3,\n                    zeroLineColor: 'rgba(0, 0, 0, 1)',\n                    drawBorder: true,\n                    color: function(context) {\n                        if (context.tick.value === 0) {\n                            return 'rgba(0, 0, 0, 1)';  // Colore per la linea dello zero\n                        }\n                        return 'rgba(0, 0, 0, 0.1)';  // Colore per le altre linee della griglia\n                    }\n                }\n            }\n        };\n\nOutputs.YAxis = config;"},"ports":[{"name":"in-Ricavi","displayName":"Ricavi","plug":"input","type":"*","group":"Inputs","index":4},{"name":"out-Data","displayName":"Data","plug":"output","type":"*","group":"Outputs","index":5},{"name":"out-YAxis","displayName":"YAxis","plug":"output","type":"*","group":"Outputs","index":6}],"children":[]},{"id":"bbaeba35-e40c-86f6-85e8-3e2e4e844b87","type":"Collection2","parameters":{},"ports":[],"children":[]},{"id":"43ca7f3d-5859-2749-c287-32fc47976067","type":"/Query/Select/Select_ricavi_noPayment","parameters":{},"ports":[],"children":[]},{"id":"4329571d-01d6-5e19-8a93-ee8003bb3be7","type":"Event Receiver","parameters":{"consume":"always","channelName":"FetchPayment"},"ports":[],"children":[]},{"id":"4583907c-93cc-8f07-45bd-0f77ca5b52b1","type":"/Query/Select/Select_ricavi_detail_incassato","parameters":{},"ports":[],"children":[]},{"id":"b2dc47c2-7936-95b4-f5eb-db60526fddea","type":"JavaScriptFunction","parameters":{"functionScript":"const profile = Inputs.Profile;\n\nconst idGestore = Inputs.Profile.id_gestore;\n\n\nif (idGestore) {\n    Outputs.Success();\n    Outputs.IdGestore = (idGes